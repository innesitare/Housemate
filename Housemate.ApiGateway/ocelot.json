{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/register",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity-api.housemate.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/auth/register",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/auth/login",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity-api.housemate.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/auth/login",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/students",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity-api.housemate.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/students",
      "UpstreamHttpMethod": [ "GET", "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/students/{email}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity-api.housemate.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/students/{email}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/students/{id:guid}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity-api.housemate.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/students/{id:guid}",
      "UpstreamHttpMethod": [ "GET", "PUT", "DELETE" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}
